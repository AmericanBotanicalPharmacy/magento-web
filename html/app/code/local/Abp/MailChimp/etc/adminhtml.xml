<config>
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <mailchimp translate="title" module="mailchimp">
                                        <title>Abp Mailchimp</title>
                                    </mailchimp>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>