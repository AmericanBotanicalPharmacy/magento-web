<?xml version="1.0"?>	
<!DOCTYPE cross-domain-policy SYSTEM "http://www.adobe.com/xml/dtds/cross-domain-policy.dtd">
<cross-domain-policy>
    <site-control permitted-cross-domain-policies="master-only"/>
    <allow-access-from domain="*.herbdoc.com"/>
    <allow-access-from domain="www.herbdoc.com"/>
    <allow-access-from domain="*.zeitbyte.com"/>
    <allow-http-request-headers-from domain="*.adobe.com" headers="SOAPAction"/>
    <allow-http-request-headers-from domain="*.zeitbyte.com" headers="SOAPAction"/>
</cross-domain-policy>